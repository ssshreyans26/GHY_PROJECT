<script>      
      console.log("inside brand")


        
      $(document).ready(function(){
          
        $('#addbrand').click(function(e) {
          e.preventDefault();
          var brandname = ($('#brandname').val());
          
        $.ajax({
          type: "POST",
          url: '/vendor_products' ,
          contentType: "application/json",
          processData: false,
            data:  JSON.stringify(Category:product,brand:brand}),
            success: function(data) {
              console.log(data)
          }
      }).done(function (data) {
          console.log("OK");
          $('#vendorproduct tr:last').after('<tr><th scope="row">1</th><td>'+data.c1+'</td><td>'+data.s1+'</td><td>'+data.b1+'</td></tr>');   
          $('#categorylist option:last').after('<option class="container" value=' + data.c1 + '/>');
          $('#subcategorylist option:last').after('<option class="container" value=' + data.s1 + '/>');     
          // $('#addedbrand').html(data.brandname)
          // $('#addedbrandrow').removeAttr("style");

      }).fail(function ( jqXHR, textStatus, errorThrown ) {
          console.log(jqXHR);
          console.log(textStatus);
          console.log(errorThrown);
        });
      });

    </script>


